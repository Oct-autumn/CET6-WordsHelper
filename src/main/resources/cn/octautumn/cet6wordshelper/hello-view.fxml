<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.Button?>

<VBox alignment="CENTER" spacing="20.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8" fx:controller="cn.octautumn.cet6wordshelper.HelloController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
   <children>
      <MenuBar>
        <menus>
          <Menu mnemonicParsing="false" text="开始">
            <items>
              <MenuItem mnemonicParsing="false" text="关闭" />
                  <MenuItem mnemonicParsing="false" text="CET6单词表设置" />
            </items>
          </Menu>
          <Menu mnemonicParsing="false" text="帮助">
            <items>
              <MenuItem mnemonicParsing="false" text="关于" />
            </items>
          </Menu>
        </menus>
      </MenuBar>
      <VBox alignment="CENTER" prefHeight="199.0" prefWidth="120.0">
         <children>
         
             <Label fx:id="welcomeText" />
             <Button onAction="#onHelloButtonClick" text="Hello!" />
         </children>
      </VBox>
   </children>
</VBox>
